{
  "name": "vscode-markdown-language-client",
  "version": "0.4.0-alpha.1",
  "description": "Export of base functionality to connect to the markdown language server",
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "sync": "npx ts-node ./sync.ts"
  },
  "keywords": [
    "vscode",
    "markdown",
    "language",
    "client"
  ],
  "author": "ProphetLamb",
  "license": "MIT",
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^20.1.2",
    "@types/progress": "^2.0.5",
    "@types/vscode": "^1.78.0",
    "progress": "^2.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vscode-languageserver-types": "^3.17.3"
  },
  "dependencies": {
    "markdown-it": "^13.0.1",
    "vscode-languageclient": "^8.1.0",
    "vscode-languageserver": "^8.1.0",
    "vscode-markdown-languageservice": "0.4.0-alpha.1",
    "vscode-uri": "^3.0.7"
  }
}
